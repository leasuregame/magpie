<%- include header %>
    <form class="form-horizontal" method="post">
    <fieldset>
        <legend>用户登录</legend>
        <div class="control-group">
            <label class="control-label" for="username">用户名</label>
            <input type="text" class="input-middle" id="username" name="username">
        </div>
        <div class="control-group">
             <label class="control-label" for="password">密码</label>
            <input type="password" class="input-middle" id="password" name="password">
        </div>

        <div class="form-actions">
        <button type="submit" class="btn btn-primary">登录</button> </div>
    </fieldset>
    </form>
<%- include footer %>
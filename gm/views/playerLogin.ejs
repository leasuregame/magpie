<%- include header %>
<form class="form-inline span4" role="form" method="post" action="/playerLogin">
    <fieldset>
        <legend>选择玩家</legend>
        <div class="control-group">
            <label class="control-group" for="playerId">玩家：</label>
            <input type="text" class="input-small" id="playerName" name="playerName">
        </div>
        <div class="form-group">
            <label for="areaId">服务器:</label>
            <select id="areaList" class="form-control input-small" name="area">

            </select>
        </div>
        <input type="text" id="target" name = "target" value = "<%=target%>">
        <script>
            $("#target").hide();
        </script>
        <button type="button" class="btn btn-primary" id="btnOK">确认</button>
    </fieldset>
</form>
 <script src="/javascripts/playerLogin.js"></script>
 </script>

<%- include footer %>
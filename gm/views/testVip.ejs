<%-include header%>
    <form class="form-inline span5" role="form">
        <fieldset>
            <legend>测试VIP</legend>

            <div class = "control-group">
                <label class="control-label" for = "VIP">VIP: </label>
                <input type="text" id = "VIP" class="input-small" >
            </div>

            <div class = "control-group">
                <label class="control-label" for = "lottery">免费抽奖+: </label>
                <input type="text" id = "lottery" class="input-small" disabled = "disabled">
            </div>

            <div class = "control-group">
                <label class="control-label" for = "friend" >好友上限+: </label>
                <input type="text" id = "friend" class="input-small" disabled = "disabled">
            </div>

            <div class = "control-group">
                <label class="control-label" for = "buyPower">体力购买+: </label>
                <input type="text" id = "buyPower" class="input-small" disabled = "disabled">
            </div>

            <div class = "control-group">
                <label class="control-label" for = "giveBless">送出祝福+: </label>
                <input type="text" id = "giveBless" class="input-small" disabled = "disabled">
            </div>

            <div class = "control-group">
                <label class="control-label" for = "receiveBless">获得祝福+: </label>
                <input type="text" id = "receiveBless" class="input-small" disabled = "disabled">
            </div>

            <div class = "control-group">
                <label class="control-label" for = "challenge">有奖竞技+: </label>
                <input type="text" id = "challenge" class="input-small" disabled = "disabled">
            </div>

            <div class = "control-group">
                <label class="control-label" for = "spiritCollect">灵气采集+: </label>
                <input type="text" id = "spiritCollect" class="input-small" disabled = "disabled">
            </div>

            <div class = "form-group">
                <button type="button" class="btn btn-primary" id = "btnOK">确定</button>
            </div>
            <div class="alert" id = "tip">

            </div>
        </fieldset>
    </form>
    <script src="/javascripts/testVip.js"></script>
    <script>
        setVip(<%- JSON.stringify(privilege) %>);
    </script>

<%-include footer%>